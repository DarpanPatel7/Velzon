@{
    ViewData["Title"] = "User Feedback";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
    var rightsModel = Functions.GetPageRightsCheck(Context.Session);
}

@section DataTableCssLoad {}
@section DataTableJsLoad {}
@section Scripts {
    <script src="@Href("~/Admin/js/PageDetails/Feedback/Feedback.js?dt="+DateTime.Now.ToString("ddMMyyyyfffttfff"))"></script>
}

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header border-bottom-dashed">
                <div class="row g-4 align-items-center">
                    <div class="col-sm">
                        <div>
                            <h5 class="card-title mb-0">Feedback List</h5>
                        </div>
                    </div>
                </div>
            </div>
            <div class="card-body">
                <table class="table table-striped table-bordered scroll-horizontal w-100 text-center nowrap dataTable no-footer" id="datatableFeedback" data-url="@Url.Content("~/Admin/GetFeedbackData")">
                    <thead class="table-light">
                        <tr class="text-uppercase">
                            <th>#</th>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Mobile</th>
                            <th>Email</th>
                            <th>Subject</th>
                            <th>Feedback</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>